  "event": [
    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Global pre-request script",
          "console.log('Making request to:', pm.request.url.toString());",
          "",
          "// Set common headers",
          "if (!pm.request.headers.has('Content-Type') && pm.request.method !== 'GET') {",
          "    pm.request.headers.add({key: 'Content-Type', value: 'application/json'});",
          "}"
        ]
      }
    },
    {
      "listen": "test",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Global test script",
          "pm.test('Response time is less than 5000ms', function () {",
          "    pm.expect(pm.response.responseTime).to.be.below(5000);",
          "});",
          "",
          "pm.test('Response has success field', function () {",
          "    try {",
          "        const response = pm.response.json();",
          "        pm.expect(response).to.have.property('success');",
          "    } catch (e) {",
          "        console.log('Response is not JSON or does not have success field');",
          "    }",
          "});",
          "",
          "// Auto-save IDs from response",
          "if (pm.response.code === 200 || pm.response.code === 201) {",
          "    try {",
          "        const response = pm.response.json();",
          "        if (response.success && response.data) {",
          "            // Save common IDs",
          "            if (response.data.id) {",
          "                const requestName = pm.info.requestName.toLowerCase();",
          "                if (requestName.includes('market')) {",
          "                    pm.environment.set('market_id', response.data.id);",
          "                } else if (requestName.includes('product')) {",
          "                    pm.environment.set('product_id', response.data.id);",
          "                } else if (requestName.includes('order')) {",
          "                    pm.environment.set('order_id', response.data.id);",
          "                }",
          "            }",
          "        }",
          "    } catch (e) {",
          "        console.log('Could not parse response for ID extraction');",
          "    }",
          "}"
        ]
      }
    }
  ],
